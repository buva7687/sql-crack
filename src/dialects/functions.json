        "REDUCE_AGG", "SET_AGG", "SET_UNION",
        "STDDEV", "STDDEV_POP", "STDDEV_SAMP",
        "VARIANCE", "VAR_POP", "VAR_SAMP",
        "CORR", "COVAR_POP", "COVAR_SAMP",
        "REGR_INTERCEPT", "REGR_SLOPE"
      ],
      "window": []
    },
    "trino": {
      "aggregates": [
        "ARRAY_AGG", "APPROX_DISTINCT", "APPROX_MOST_FREQUENT", "APPROX_PERCENTILE", "APPROX_SET",
        "ARBITRARY", "BOOL_AND", "BOOL_OR",
        "CHECKSUM", "EVERY",
        "GEOMETRIC_MEAN", "HISTOGRAM",
        "MAP_AGG", "MAP_UNION", "MAP_UNION_SUM", "MULTIMAP_AGG",
        "REDUCE_AGG", "SET_AGG", "SET_UNION",
        "LISTAGG",
        "STDDEV", "STDDEV_POP", "STDDEV_SAMP",
        "VARIANCE", "VAR_POP", "VAR_SAMP",
        "CORR", "COVAR_POP", "COVAR_SAMP",
        "REGR_INTERCEPT", "REGR_SLOPE",

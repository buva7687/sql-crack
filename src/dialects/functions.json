        "VARIANCE", "VAR_POP", "VAR_SAMP",
        "CORR", "COVAR_POP", "COVAR_SAMP",
        "REGR_AVGX", "REGR_AVGY", "REGR_COUNT", "REGR_INTERCEPT",
        "REGR_R2", "REGR_SLOPE", "REGR_SXX", "REGR_SXY", "REGR_SYY"

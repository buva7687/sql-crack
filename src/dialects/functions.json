        "ARRAY_AGG", "APPROX_DISTINCT", "APPROX_PERCENTILE",
        "ARBITRARY", "BOOL_AND", "BOOL_OR",
        "CHECKSUM", "EVERY",
        "GEOMETRIC_MEAN", "HISTOGRAM",
